<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingsItemView">
        <attr name="setting_title" format="string"/>
        <attr name="setting_icon" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TransactionInfoItemView">
        <attr name="title" format="string"/>
        <attr name="value" format="string"/>
        <attr name="valueSubtitle" format="string"/>
        <attr name="showValueBackground" format="boolean"/>
        <attr name="valueIcon" format="integer"/>
    </declare-styleable>
    <declare-styleable name="FullTransactionInfoItemView">
        <attr name="infoTitle" format="string"/>
        <attr name="infoValue" format="string"/>
        <attr name="infoValueIcon" format="integer"/>
    </declare-styleable>
</resources>
